{
  "name": "RecipeBook",
  "version": "0.0.1",
  "description": "Recipe book server app",
  "main": "index.js",
  "scripts": {
    "test": "mocha --opts .mocha.opts src/**/*.test.js",
    "test:integration": "mocha --opts .mocha.opts src/**/*.int.js",
    "test:all": "mocha --opts .mocha.opts src/**/*.test.js src/**/*.int.js",
    "test:watch": "mocha --watch --opts .mocha.opts src/**/*.test.js",
    "start": "node server.js",
    "prestart": "flock migrate",
    "predev": "npm run dev:migrate",
    "dev": "node -r dotenv/config server.js",
    "predev:watch": "npm run dev:migrate",
    "dev:watch": "nodemon -r dotenv/config server.js",
    "dev:migrate": "flock -r dotenv/config migrate",
    "dev:rollback": "flock -r dotenv/config rollback",
    "migrate:create": "flock create"
  },
  "keywords": [],
  "author": "Julia Ricketti <julsricketti@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "dotenv": "6.2.0",
    "eslint": "5.3.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "mocha": "5.2.0",
    "nodemon": "1.18.3"
  },
  "dependencies": {
    "@feathersjs/errors": "3.3.0",
    "@feathersjs/express": "1.2.3",
    "@feathersjs/feathers": "3.1.7",
    "@gradealabs/flock": "github:launchfort/flock#3.2.1",
    "@gradealabs/flock-pg": "github:launchfort/flock-pg",
    "aws-sdk": "2.406.0",
    "compression": "1.7.3",
    "cors": "2.8.4",
    "feathers-authentication-hooks": "0.3.0",
    "feathers-hooks-common": "4.16.0",
    "feathers-sequelize": "3.1.2",
    "flock": "0.3.10",
    "helmet": "3.13.0",
    "lodash": "4.17.11",
    "moment-timezone": "0.5.21",
    "multer": "1.4.1",
    "pg": "7.4.3",
    "pg-copy-streams": "1.2.0",
    "platform-user": "github:wearedragonfly/platform-user#0.0.4",
    "pm2": "3.2.9",
    "request": "2.88.0",
    "request-promise-native": "1.0.7",
    "sequelize": "4.38.0",
    "winston": "3.0.0",
    "xlsx": "0.13.3"
  },
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.0"
  }
}
